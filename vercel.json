{
 "version": 2,
 "builds": [
  {
   "src": "./index.js",
   "use": "@vercel/node"
  },
  {
   "src": "static/**",
   "use": "@vercel/static"
  }
 ],
 "routes": [
  {
   "src": "/(.*)",
   "dest": "/"
  },
  {
   "src": "/(.+\\.[a-z]+)$",
   "dest": "/static/$1"
  }
 ],
 "github": {
  "silent": true
 }
}
