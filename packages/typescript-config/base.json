{
 "$schema": "https://json.schemastore.org/tsconfig",
 "compilerOptions": {
  "lib": ["dom", "dom.iterable", "esnext"],
  "skipLibCheck": true,
  "strict": true,
  "esModuleInterop": true,
  "module": "esnext",
  "moduleResolution": "bundler",
  "resolveJsonModule": true,
  "isolatedModules": true,
  "incremental": true,
  "paths": {
   "@/*": ["./*"]
  },
  "target": "ES2022"
 }
}
